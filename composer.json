{
    "name": "slipstream/vault-client",
    "type": "library",
    "description": "HashiVault client",
    "license": "proprietary",
    "require": {
        "php": "^7.4.0",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "ext-json": "*",
        "guzzlehttp/guzzle": "^2.0 || ^6.0"
    },
    "require-dev": {
        "psr/log": "^1.1",
        "friendsofphp/php-cs-fixer": "^2.16.1",
        "phpstan/phpstan-phpunit": "^0.12",
        "phpstan/phpstan": "^0.12",
        "phpstan/phpstan-strict-rules": "^0.12",
        "squizlabs/php_codesniffer": "^3.5",
        "phpunit/phpunit": "^8.5"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "Vault\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    }
}
